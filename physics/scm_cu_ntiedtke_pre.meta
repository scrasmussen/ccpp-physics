  kind = kind_phys
  intent = in
  optional = F
[con_rd]
  standard_name = gas_constant_dry_air
  long_name = gas constant dry air
  units = J kg-1 K-1
  dimensions = ()
  type = real | kind = kind_phys
  intent = in
  optional = F
[con_rv]
